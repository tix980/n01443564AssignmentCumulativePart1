@model IEnumerable<n01443564AssignmentCumulativePart1.Models.Class>
@{
    ViewBag.Title = "List";
}

<h2>List of classes</h2>
<form action="/Class/List" method="GET">
    <input type="text" name="SearchKey" />
    <input type="button" value="Search" />
</form>
<span style="font-style:italic;">@Model.Count() Result(s)</span>
@foreach (var Indivclass in Model){
    <a href="/Class/Show/@Indivclass.ClassId"><div>@Indivclass.ClassCode : @Indivclass.ClassName </div></a>
}


