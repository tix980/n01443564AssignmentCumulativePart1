@model IEnumerable<n01443564AssignmentCumulativePart1.Models.Student>
@{
    ViewBag.Title = "List";
}
<form action="/Student/List" method="GET">
    <input type="text" name="SearchKey" />
    <input type="submit" value="Search" />
</form>
<span style="font-style:italic;">@Model.Count() Result(s)</span>
<h2>List of Students</h2>
@foreach (var student in Model)
{
    <a href="/Student/Show/@student.StudentId"><div>@student.StudentFname,@student.StudentLname</div></a>
}

